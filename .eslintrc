{
  "parser": "@babel/eslint-parser",
  "extends": "eslint-config-deloitte-react",
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "plugins": [
    "jasmine"
  ],
  "rules": {
    "linebreak-style": 0
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false
  }
}
